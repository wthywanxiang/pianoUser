{
	"easycom": {
		"^tn-(.*)": "@/tuniao-ui/components/tn-$1/tn-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index",
			"style": {
				"navigationBarTitleText": "加载中...",
				"enablePullDownRefresh": false
			}

		},
		{
			"path": "pages/user/user",
			"style": {
				"navigationBarTitleText": "个人中心",
				"enablePullDownRefresh": true
			}

		},
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "排行"
			}
		}, {
			"path": "pages/room/room",
			"style": {
				"navigationBarTitleText": "预约",
				"enablePullDownRefresh": true
			}

		},
		{
			"path": "pages/history/history",
			"style": {
				"navigationBarTitleText": "预约记录",
				"enablePullDownRefresh": true
			}

		}, {
			"path": "pages/register/register",
			"style": {
				"navigationBarTitleText": "用户登录",
				"enablePullDownRefresh": false
			}

		}
	    ,{
            "path" : "pages/signIn/signIn",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {},
	"tabBar": {
		"list": [{
			"iconPath": "static/tabbar/im-contacts.png",
			"text": "我的",
			"selectedIconPath": "static/tabbar/im-contacts_active.png",
			"pagePath": "pages/user/user"
		}, {
			"iconPath": "static/tabbar/rank.png",
			"text": "排行",
			"selectedIconPath": "static/tabbar/rankactive.png",
			"pagePath": "pages/index/index"
		}]
	}
}
